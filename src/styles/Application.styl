/**
 * Eurostile
 */
@font-face
  font-family Eurostile
  src url(assets/fonts/73ff8a514a9a68d88a694f6c543f15a2.woff2) format('woff2')
  font-weight 400
  font-style normal


html, body
  width 100%
  height 100%
  margin 0
  padding 0
  background $BACKGROUND_COLOR
  color $TEXT_COLOR
  font-family Eurostile,Helvetica,sans-serif
  display flex

#Application
  display flex
  width 100%
  height 100%

/**
 * Estilos generales
 */
a
  text-decoration none
  color inherit

a:hover
  color $MAIN_COLOR
